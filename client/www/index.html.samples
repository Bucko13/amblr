<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width">
    <title></title>

    <link href="lib/ionic/css/ionic.css" rel="stylesheet">
    <link href="css/style.css" rel="stylesheet">

    <!-- IF using Sass (run gulp sass first), then uncomment below and remove the CSS includes above
    <link href="css/ionic.app.css" rel="stylesheet">
    -->

    <!-- ionic/angularjs js -->
    <script src="lib/ionic/js/ionic.bundle.js"></script>

    <!-- cordova script (this will be a 404 during development) -->
    <script src="cordova.js"></script>

    <!-- your app's js -->
    <script src="js/app.js"></script>
  </head>
  <body ng-app="starter">

    <ion-pane>
      <ion-header-bar class="bar-stable">
        <h1 class="title">Hello World</h1>

      </ion-header-bar>
      <ion-content>
        <button class='button button-positive'>I am a button</button>

        <p><button class='button button-positive icon ion-ionic icon-left'>icon
        </button>
        </p>
        <p>
          <i class='icon ion-navicon'> hamburger icon</i>
        </p>
      <div class='card'>
      Hello
      </div>

      <div class='list list-inset'>Hello
      </div>

      <div class='list item-image'>

      </div>

      <div class='item item-icon-left item-text-wrap'>
         <i class='icon ion-home'></i>
         Lorem ipsum.....
      </div>

      <ion-list>
         <ion-item>hello</ion-item>
      </ion-list>

      <div class='list'>
         <label class='item item-input'>
            <span class='input-label'>First Name</span>
            <input type='text' placeholder='First Name'></input>
         </label>
         <label class='item item-input'>
            <span class='input-label'>Last Name</span>
            <input type='text' placeholder='Last Name'></input>
         </label>
      </div>

      <div class='list list-inset'>
         <label class='item item-input'>
            <span class='input-label'>First Name</span>
            <input type='text' placeholder='First Name'></input>
         </label>
         <label class='item item-input'>
            <span class='input-label'>Last Name</span>
            <input type='text' placeholder='Last Name'></input>
         </label>
         <div class='item item-input-inset'>
            <label class='item-input-wrapper'>
                <span>Hello</span>
            </label>
         </div>
      </div>

      <div class='item range'>
         <span class='input-lable'>Feeling</span>
         <i class='icon ion-sad-outline'></i>
         <input type='range' name='feeling' min='0' max='100' value='50'>
         <i class='icon ion-happy-outline'></i>

      </div>

      <div class='item item-input item-select'>
         <div class='input-label'>Favorite Pet</div>
         <select>
            <option>Dogs</option>
            <option>Cats</option>
            <option>Llamas</option>
         </select>
      </div>

      </ion-content>
      <ion-tabs class="tabs-positive tabs-icon-top">

        <ion-tab title="Home" icon-on="ion-ios-filing" icon-off="ion-ios-filing-outline">
          <!-- Tab 1 content -->
        </ion-tab>

        <ion-tab title="About" icon-on="ion-ios-clock" icon-off="ion-ios-clock-outline">
          <!-- Tab 2 content -->
        </ion-tab>

        <ion-tab title="Settings" icon-on="ion-ios-gear" icon-off="ion-ios-gear-outline">
          <!-- Tab 3 content -->
        </ion-tab>

      </ion-tabs>

    </ion-pane>
  </body>
</html>
